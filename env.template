# Foundry Smart Contract Configuration
# Copy this file to .env and fill in the values after deployment

# Sui Network Configuration
SUI_NETWORK=testnet

# Package ID (Obtained after deployment)
# Example: 0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef
PACKAGE_ID=

# Object Types (Full type identifiers)
# Format: PACKAGE_ID::module_name::StructName

# Project Object Type
# Example: 0x1234...::foundry::Project
PROJECT_TYPE=

# Contribution Object Type  
# Example: 0x1234...::foundry::Contribution
CONTRIBUTION_TYPE=

# Feedback Object Type
# Example: 0x1234...::foundry::Feedback
FEEDBACK_TYPE=

# Walrus Configuration (Testnet)
WALRUS_PUBLISHER_URL=https://publisher.walrus-testnet.walrus.space
WALRUS_AGGREGATOR_URL=https://aggregator.walrus-testnet.walrus.space

# Frontend Configuration
VITE_SUI_NETWORK=testnet
VITE_PACKAGE_ID=${PACKAGE_ID}
VITE_PROJECT_TYPE=${PROJECT_TYPE}
VITE_CONTRIBUTION_TYPE=${CONTRIBUTION_TYPE}
VITE_FEEDBACK_TYPE=${FEEDBACK_TYPE}
VITE_WALRUS_PUBLISHER_URL=${WALRUS_PUBLISHER_URL}
VITE_WALRUS_AGGREGATOR_URL=${WALRUS_AGGREGATOR_URL}

